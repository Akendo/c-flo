  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-noflo-manifest": "^0.1.11",
    "grunt-yamllint": "^0.2.0"
  },
  "dependencies": {
    "msgflo": "^0.5.13",
    "msgflo-nodejs": "^0.4.10",
    "noflo": "^0.5.14",
    "noflo-core": "^0.1.14",
    "noflo-runtime-msgflo": "^0.1.13"
  },
  "msgflo": {
    "variables": {
      "NOFLO": "./node_modules/.bin/noflo-runtime-msgflo --name #ROLE --graph #COMPONENT",
      "FOREIGN": "./node_modules/.bin/msgflo-register-foreign participants/#ROLE.yml"
    },
    "components": {
      "c-flo/DetectPartyMods": "#NOFLO",
      "c-flo/logger": "#NOFLO",
      "c-flo/ampel-nerd": "#FOREIGN",
      "c-flo/barstatus-bar": "#FOREIGN",
      "c-flo/barstatus-c-lab": "#FOREIGN",
      "c-flo/barstatus-nerd": "#FOREIGN",
      "c-flo/c-beam": "#FOREIGN",
      "c-flo/c_leuse": "#FOREIGN",
      "c-flo/he1": "#FOREIGN",
      "c-flo/ingress-table": "#FOREIGN",
      "c-flo/mechblast": "#FOREIGN",
      "c-flo/nerdctrl": "#FOREIGN",
      "c-flo/siri": "#FOREIGN"
    }
  },
  "noflo": {
    "components": {
      "DetectPartyMods": "components/DetectPartyMods.coffee"
    }
  }
}